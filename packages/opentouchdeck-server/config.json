{
  "plugins": [
    {
      "name": "OBS WebSocket",
      "data": {
        "address": "localhost",
        "port": 4444,
        "password": null
      }
    }
  ],
  "pages": [
    {
      "title": "Page One",
      "buttons": [
        {
          "position": 0,
          "faicon": "fa fa-user",
          "action": {
            "name": "Counter",
            "params": {
              "type": "increment",
