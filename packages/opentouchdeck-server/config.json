              },
              "UI": {
                "faicon": "fab fa-mixer"
              }
            },
            {
              "event": "twitch/chat",
              "data": {
                "channel": "#northantara",
                "message": "twitch"
              },
              "UI": {
                "faicon": "fab fa-twitch"
              }
            }
          ]
        }
